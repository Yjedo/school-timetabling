<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.yxw.managesystem.mapper.ClazzMapper">
    <select id="selectAll" resultType="com.yxw.managesystem.entity.Clazz">
        SELECT * FROM clazz
        WHERE problem_id = #{problemId}
    </select>
    <update id="empty">
        TRUNCATE TABLE clazz
    </update>
</mapper>
